  "keywords": ["contentful", "cms", "sdk", "symfony2"],
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": ">=5.5.9",
    "contentful/contentful": "~0.6.2@beta",
    "symfony/framework-bundle": "~2.7|~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },
  "autoload": {
    "psr-4": {
      "Contentful\\ContentfulBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}
